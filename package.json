  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiverr/page-timing.git"
  },
  "homepage": "https://github.com/fiverr/page-timing#readme",
  "main": "src/index.js",
  "browser": "dist/main.js",
  "scripts": {
